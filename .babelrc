{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-react-remove-prop-types",
      "transform-class-properties",
      {
        "mode": "wrap"
      }
    ]
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    },
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "test:tdd": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
